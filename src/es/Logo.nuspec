		<owners>kzu</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Aprende a programar con Xamarin Workbooks, C# y la legendaria Tortuga!</description>
		<copyright>Copyright © 2016 Daniel Cazzulino </copyright>
		<licenseUrl>https://raw.githubusercontent.com/kzu/Logo/master/LICENSE</licenseUrl>
		<projectUrl>https://github.com/kzu/Logo</projectUrl>
		<tags>xamarin workbooks logo smallbasic linqpad-samples</tags>
		<dependencies>
			<group targetFramework=".NETFramework4.5">
				<dependency id="SmallBasic.Workbook" version="1.2.0" exclude="Build,Analyzers" />
